set smoothscroll
let scrollstep = 120
let barposition = "bottom"

" swap H/L and J/K (I'm picky)
unmap H
unmap L
map H previousTab
map L nextTab

unmap J
unmap K
map J goBack
map K goForward

let blacklists = ["https://www.nytimes.com/crosswords/game/*", "https://mail.google.com/*", "*://mail.google.com/*", "@https://mail.google.com/mail/*"]
