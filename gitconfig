	name = Oliver Newman
	email = orn688@gmail.com
[push]
	default = simple
[fetch]
        prune = true
[credential]
	helper = osxkeychain
[core]
	excludesfile = ~/.gitignore_global
[alias]
	graph = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset%n' --abbrev-commit --date=relative --branches
        b = branch
	c = commit
	cm = commit -m
	ca = commit --amend
	a = add
	s = status
	co = checkout
	unstage = reset HEAD --
	last = log -1 HEAD
	root = rev-parse --show-toplevel
